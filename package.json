{
  "name": "connectio",
  "version": "2.0.0",
  "description": "Video conferencing application with WebRTC",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "peerjs": "peerjs --port 3030",
    "peerjs-dev": "peerjs --port 3030 --auto-cleanup",
    "all": "concurrently \"npm run peerjs\" \"npm start\""
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^4.22.1",
    "express-session": "^1.18.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "peer": "^1.0.2",
    "socket.io": "^4.8.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.22"
  }
}
